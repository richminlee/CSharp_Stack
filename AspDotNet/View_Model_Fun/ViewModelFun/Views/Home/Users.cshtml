@model List<User>

<div class="container">
    <div class="row">
        <div class="col-3"></div>
        <div class="col-6 text-center m-4">
            <h2>Here are some Users!</h2>
        </div>
    </div>
    <div class="row">
        <div class="col-4"></div>
        <div class="col-4 border border-dark text-center m-4">
            @foreach(var person in Model)
            {
                <h3>@person.FirstName @person.LastName</h3>;
            }
        </div>
    </div>
</div> 
